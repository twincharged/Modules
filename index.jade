doctype html

html(lang="en")
  head
    title Modules
    meta(charset="utf-8")
    meta(name="description" content="")
    link(href="css/grid.min.css" rel="stylesheet")
    link(href="css/hover-transitions.css" rel="stylesheet")
    link(href="css/demo.css" rel="stylesheet")
  body
    section#hover-transitions
      .container
        .row
          .col-12
            .liquid
              svg(height="60" width="320" xmlns="http://www.w3.org/2000/svg")
                rect(height="60" width="320")
                span Add some Zest
        .row
          .col-3: a.box.zest-outer(href="#") Outer
          .col-3: a.box.zest-target(href="#") Target
          .col-3: a.box.zest-bounce(href="#") Bounce
          .col-3: a.box.zest-corners(href="#") Corners
        .row
          .col-3: a.box.zest-expand(href="#") Expand
          .col-3: a.box.zest-contract(href="#") Contract
          .col-3: a.box.zest-focus(href="#") Focus
          .col-3: a.box.zest-enclose(href="#") Enclose



    section#vectors
      .container
        .row
          .col-12
            .liquid
              svg(height="60" width="320" xmlns="http://www.w3.org/2000/svg")
                rect(height="60" width="320")
                span Vector Magic
      .container
        .row
          .col-6.vect: include ./vectors/code.jade
          .col-6.vect: include ./vectors/ruler.jade
        .row
          .col-12.vect: include ./vectors/thr.jade
        .row
          .col-12.vect: include ./vectors/mac.jade
        .row
          .col-6.vect: include ./vectors/ipad.jade
          .col-6.vect: include ./vectors/iphone.jade
        .row
          .col-6.vect: include ./vectors/pencil.jade
          .col-6.vect: include ./vectors/server.jade




    script(src="js/svg-draw.js")
